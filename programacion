#include <iostream>
#include <string>
#include <cstdlib>
#include<ctime>

using namespace std;

bool Esmultiplode3 (int n)
{
	return n%3 == 0;
}
int main(){
int num;
string  msg="no es multiple";
cout<<"ingrese un numero"<<endl;
cin>>num;

if(Esmultiplode3(num))
{
	msg="Es Multiplo";
}
cout<<msg<<endl;
return 0;
}

